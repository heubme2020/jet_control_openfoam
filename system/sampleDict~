FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

interpolationScheme cellPoint;

setFormat       raw;

sets
(
    horizontalLine
    {
        type    uniform;
        axis    y;
        start   ( 0.001 0.05 0.005 );
        end     ( 0.099 0.05 0.005 );
        nPoints 100;
    }
);

fields          ( p U );


